;FUNCTION PLANTSEARCH 
;FRAGT DEN BENUTZER, OB ER/SIE EINE BESTIMMTE PFLANZE ODER EINE LISTE ALLER PFLANZEN FINDEN MÖCHTE
;@PARAM LIS: {LIST}, LISTE MIT ALLEN INFORMATIONEN ZU DEN PFLANZEN, INFEKTIONSKRANKHEITEN,  EIGENSCHAFTEN UND THERAPIEZIELE.
(DEFUN PLANTSEARCH (LIS)
	(FORMAT T "~%========================================") 
	(FORMAT T "~%GEBEN SIE DEN NAMEN DER PFLANZE EIN, UM DIE DETAILLIERTE BESCHREIBUNG ZU ERHALTEN.")
	(FORMAT T "~%WENN DER NAME AUS MEHREREN WORTEN BESTEHT VERBINDEN SIE DIESE DURCH EINEN BINDESTRICH.")
	(FORMAT T "~%WENN SIE ZUM HAUPTMENUE ZURUECKKEHREN WOLLEN, GEBEN SIE HP EIN!")
	(FORMAT T "~%WENN SIE DEN NAMEN DER PFLANZE NICHT WISSEN SCHAUEN SIE SICH DIE VOLLSTAENDIGE LISTE ALLER VERFUEGBAREN PFLANZEN AN, DIE SIE DURCH EINGABE VON * ERHALTEN.")
	(FORMAT T "~%----------------------------------------~%")
	(SETQ PLANTNAME (READ))
	(COND ((EQUAL PLANTNAME '* )(PRINTALL 'NAME PLANTS)(PLANTSEARCH PLANTS))
		((EQUAL PLANTNAME 'HP)(START))
		(T (PRINTPLANT PLANTNAME LIS))))

;FUNCTION INFEKTIONSKRANKHEITSEARCH
;FRAGT DEN BENUTZER, OB ER/SIE EINEN BESTIMMTEN VIRUS, SYMPTOME, KRANKHEIT ODER BAKTERIE SUCHEN MÖCHTE
;@PARAM LIS: {LIST}, LISTE MIT ALLEN INFORMATIONEN ZU DEN PFLANZEN, INFEKTIONSKRANKHEITEN, EIGENSCHAFTEN UND THERAPIEZIELE.
(DEFUN INFEKTIONSKRANKHEITSEARCH (LIS) 
	(FORMAT T "~%========================================")	
	(FORMAT T "~%WAEHLEN SIE AUS:") 
	(FORMAT T "~%1. SIE KENNEN DEN NAMEN DER KRANKHEIT.")
	(FORMAT T "~%2. SIE KENNEN DEN NAMEN DES BAKTERIUMS, DAS DIE KRANKHEIT VERURSACHT.")
	(FORMAT T "~%3. SIE KENNEN DEN NAMEN DES VIRUS, DER DIE KRANKHEIT VERURSACHT.")
	(FORMAT T "~%4. SIE KENNEN NUR DIE SYMPTOME DER KRANKHEIT.")
	(FORMAT T "~%5. ZURUECK ZUM HAUPTMENUE.")
	(FORMAT T "~%----------------------------------------~%")
	(SETQ INFOPT (READ))
  	(COND((EQUAL INFOPT '1)(INFEKTIONSKRANKHEITSEARCH-KRANKHEIT LIS))
       ((EQUAL INFOPT '2)(INFEKTIONSKRANKHEITSEARCH-BAKTERIEN LIS))
       ((EQUAL INFOPT '3)(INFEKTIONSKRANKHEITSEARCH-VIREN LIS))
	   ((EQUAL INFOPT '4)(INFEKTIONSKRANKHEITSEARCH-SYMPTOM LIS))
	   ((EQUAL INFOPT '5)(START))
       (T (FORMAT T "~%DAS IST KEIN GUELTIGER BEFEHL, BITTE VERSUCHEN SIE NOCHMAL~%")(INFEKTIONSKRANKHEITSEARCH PLANTS))))

