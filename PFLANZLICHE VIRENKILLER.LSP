;FUNCTION PRINTPLANT 
;SUCHT DEN ANGEGEBENEN NAMEN IN DER LISTE DER MÖGLICHEN PFLANZENNAMEN UND DRUCKT DIE GEFUNDENEN INFORMATIONEN AUS
;@PARAM NAME: {SYMBOL}, NAME DER GESUCHTEN PFLANZE
;@PARAM LIS: {LIST}, LISTE MIT ALLEN INFORMATIONEN ZU DEN PFLANZEN, INFEKTIONSKRANKHEITEN,  EIGENSCHAFTEN UND THERAPIEZIELE.
(DEFUN PRINTPLANT (NAME LIS)
	(COND((NULL LIS)(FORMAT T "~%========================================~%KEINE PFLANZE MIT DIESEM NAMEN GEFUNDEN~%")(PLANTSEARCH PLANTS))
		((MEMBER NAME (CDR(ASSOC 'NAME (CAR LIS))))(FORMAT T "~%========================================~%~A (WISSENSCHAFTLICHER NAME ~A)~%"(SECOND (ASSOC 'NAME (CAR LIS)))(THIRD (ASSOC 'NAME (CAR LIS))))
											  (FORMAT T "~%ES BEKAEMPFT FOLGENDEN KRANKHEITEN: ~{~A~^, ~}.~%"  (CDR (ASSOC 'INFEKTIONSKRANKHEITEN (CAR LIS))))
											  (FORMAT T "~%ES BEKAEMPFT FOLGENDEN BAKTERIEN: ~{~A~^, ~}.~%" (CDR (ASSOC 'BAKTERIEN (CAR LIS))))
											  (FORMAT T "~%ES BEKAEMPFT FOLGENDEN VIREN: ~{~A~^, ~}.~%" (CDR (ASSOC 'VIREN (CAR LIS))))
									          (FORMAT T "~%ES HAT FOLGENDE EIGENSCHAFTEN: ~{~A~^, ~}.~%" (CDR (ASSOC 'EIGENSCHAFTEN (CAR LIS))))
											  (FORMAT T "~%ES VERRINGERT: ~{~A~^, ~}.~%" (CDR(ASSOC 'VERRINGERN (CAR LIS))))
											  (FORMAT T "~%ES VERBESSERT: ~{~A~^, ~}.~%" (CDR(ASSOC 'VERBESSERN (CAR LIS)))))
		(T(PRINTPLANT NAME (CDR LIS)))))
        
;FUNCTION PRINTZIEL 
;SUCHT DEN ANGEGEBENEN NAMEN IN DER LISTE DER MÖGLICHEN ZIELEN UND DRUCKT ALLER PFLANZEN, DIE DIESES ZIEL ERREICHEN KÖNNEN
;@PARAM TYPE: {SYMBOL}, TYP DES ZIELS ('VERRINGERN ODER 'VERBESSERN)
;@PARAM NAME: {SYMBOL}, NAME DES GESUCHTEN ZIELS
;@PARAM LIS: {LIST}, LISTE MIT ALLEN INFORMATIONEN ZU DEN PFLANZEN, INFEKTIONSKRANKHEITEN,  EIGENSCHAFTEN UND THERAPIEZIELE.
(DEFUN PRINTZIEL (TYPE NAME LIS &OPTIONAL RESULT)
	(COND((AND (EQL TYPE 'VERRINGERN)(NULL LIS) RESULT)(FORMAT T "~%========================================~%~A WIRD DURCH FOLGENDE PFLANZEN VERRINGERT: ~{~A~^, ~}.~%" NAME RESULT))
		 ((AND (EQL TYPE 'VERBESSERN)(NULL LIS) RESULT)(FORMAT T "~%========================================~%~A WIRD DURCH FOLGENDE PFLANZEN VERBESSERT: ~{~A~^, ~}.~%" NAME RESULT))
		 ((EQUAL NAME 'HP)(START))
		 ((EQUAL NAME '*)(PRINTALL 'ZIELE PLANTS )(ZIELSEARCH PLANTS))
		 ((AND (NULL LIS)(EQL TYPE 'VERRINGERN))(FORMAT T "~%========================================~%~A ZU ~A WURDE IN KEINEM PFLANZLICHEN THERAPIEZIEL GEFUNDEN~%" NAME TYPE)(ZIELSEARCH-VERRINGERN PLANTS))
		 ((AND (NULL LIS)(EQL TYPE 'VERBESSERN))(FORMAT T "~%========================================~%~A ZU ~A WURDE IN KEINEM PFLANZLICHEN THERAPIEZIEL GEFUNDEN~%" NAME TYPE)(ZIELSEARCH-VERBESSERN PLANTS))
		 ((MEMBER NAME (ASSOC TYPE (CAR LIS)))(PRINTZIEL TYPE NAME (CDR LIS)(NCONC RESULT (LIST (SECOND(ASSOC 'NAME (CAR LIS)))))))	
		 (T (PRINTZIEL TYPE NAME (CDR LIS) RESULT)))) 

;FUNCTION PRINTEIGENSCHAFTEN 
;SUCHT DEN ANGEGEBENEN NAMEN IN DER LISTE DER MÖGLICHEN EIGENSCHAFTEN UND DRUCKT ALLER PFLANZEN, DIE DIESE EIGENSCHAFT HABEN
;@PARAM NAME: {SYMBOL}, NAME DES GESUCHTEN EIGENSCHAFT
;@PARAM LIS: {LIST}, LISTE MIT ALLEN INFORMATIONEN ZU DEN PFLANZEN, INFEKTIONSKRANKHEITEN,  EIGENSCHAFTEN UND THERAPIEZIELE.
;@PARAM RESULT: {LIST}, LISTE ALLER PFLANZEN, DIE DIESE EIGENSCHAFT HABEN 
(DEFUN PRINTEIGENSCHAFTEN (NAME LIS &OPTIONAL RESULT)
      (COND((AND (NULL LIS) RESULT)(FORMAT T "~%========================================~%DIE EIGENSCHAFT ~A HABEN DIE FOLGENDEN PFLANZEN:~%~{~A~^, ~}.~%" NAME RESULT))
		((NULL LIS)(FORMAT T "~%========================================~%KEINE EIGENSCHAFT MIT DIESEM NAMEN GEFUNDEN~%")(EIGENSCHAFTENSEARCH PLANTS))
		((FIND NAME (ASSOC 'EIGENSCHAFTEN (CAR LIS)))(PRINTEIGENSCHAFTEN NAME (CDR LIS)(NCONC RESULT (LIST (SECOND (ASSOC 'NAME (CAR LIS)))))))
		(T (PRINTEIGENSCHAFTEN NAME (CDR LIS) RESULT))))

;FUNCTION PRINTKRANKHEIT 
