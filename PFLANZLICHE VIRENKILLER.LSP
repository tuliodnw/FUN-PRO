;FUNCTION PRINTALL 
;SUCHT DEN ANGEGEBENEN TYP UND DRUCKT ALLE PFLANZEN, VIREN, EIGENSCHAFTEN ODER THERAPEUTISCHEN ZIELE AUS, DIE IN DER LISTE VORHANDEN SIND.
;@PARAM TYPE: {SYMBOL}, NAME DES GESUCHTEN TYP ('ZIELE, 'EIGENSCHAFTEN, 'NAME; 'INFEKTIONSKRANKHEITEN, 'BAKTERIEN  ODER 'VIREN).
;@PARAM LIS: {LIST}, LISTE MIT ALLEN INFORMATIONEN ZU DEN PFLANZEN, INFEKTIONSKRANKHEITEN,  EIGENSCHAFTEN UND THERAPIEZIELE.
(DEFUN PRINTALL (TYPE LIS)
(LET*((NAMEN (APPLY #'APPEND (MAPCAR (LAMBDA (ROW) (CDR (ASSOC 'NAME ROW))) PLANTS))))
(COND((EQL TYPE 'NAME) (FORMAT T "~%DIE VERFUEGBAREN PFLANZEN SIND")(FORMAT T "~%~23A|~23A" "DEUTSCH" "LATEINISCH" )(FORMAT T "~%~23A|~23A" "-----------------------" "-----------------------")(MAPC #'(LAMBDA (DE LAT) (FORMAT T "~%~23:@<~D~>|~23:@<~A~>" DE LAT)) (REMOVE NIL(MAPLIST (LAMBDA(X) (IF (EVENP (LENGTH X)) (CAR X))) NAMEN)) (REMOVE NIL(MAPLIST (LAMBDA(X) (IF (ODDP (LENGTH X)) (CAR X))) NAMEN)))(FORMAT T "~%"))
((EQL TYPE 'VIREN) (FORMAT T "~%ALLE IN DER DATENBASIS ENTHALTENEN VIREN SIND:~%~{~A~^, ~}~%" (REMOVE-DUPLICATES (APPLY #'APPEND (MAPCAR (LAMBDA (ROW) (CDR (ASSOC 'VIREN ROW))) LIS)))))
((EQL TYPE 'INFEKTIONSKRANKHEITEN) (FORMAT T "~%ALLE KRANKHEITEN DER DATENBASIS SIND:~%~{~A~^, ~}~%"  (REMOVE-DUPLICATES (APPLY #'APPEND (MAPCAR (LAMBDA (ROW) (CDR (ASSOC 'INFEKTIONSKRANKHEITEN ROW))) LIS)))))
((EQL TYPE 'BAKTERIEN) (FORMAT T "~%ALLE IN DER DATENBASIS ENTHALTENEN BAKTERIEN SIND:~%~{~A~^, ~}~%" (REMOVE-DUPLICATES (APPLY #'APPEND (MAPCAR (LAMBDA (ROW) (CDR (ASSOC 'BAKTERIEN ROW))) LIS)))))
((EQL TYPE 'VERRINGERN) (FORMAT T "~%ALLE IN DER DATENBASIS ENTHALTENEN SYMPTOME SIND:~%~{~A~^, ~}~%" (REMOVE-DUPLICATES (APPLY #'APPEND (MAPCAR (LAMBDA (ROW) (CDR (ASSOC 'VERRINGERN ROW))) LIS)))))
((EQL TYPE 'ZIELE) (FORMAT T "~%DIE VERFUEGBAREN THERAPIEZIELE SIND:~%~%1. VERRINGERN VON ~{~A~^, ~}.~%~%2. VERBESSERN VON ~{~A~^, ~}.~%"(REMOVE-DUPLICATES (APPLY #'APPEND (MAPCAR (LAMBDA (ROW) (CDR (ASSOC 'VERRINGERN ROW))) LIS))) (REMOVE-DUPLICATES (APPLY #'APPEND (MAPCAR (LAMBDA (ROW) (CDR (ASSOC 'VERBESSERN ROW))) LIS)))))
(T (FORMAT T "~%ALLE VERFUEGBAREN ~A SIND:~%~{~A~^, ~}.~%" TYPE (REMOVE-DUPLICATES (APPLY #'APPEND (MAPCAR (LAMBDA (ROW) (CDR (ASSOC TYPE ROW))) LIS))))))))

;FUNCTION PRINTKEY 
;RUFT DIE FUNKTION PRINTKEY-HELPER MIT DER LISTE ALLER VERFÜGBAREN SYMBOLE EINES TYPS AUF
;@PARAM KEYWORD: {SYMBOL}, GESUCHTEN STICHWORT
;@PARAM LIS: {LIST}, LISTE MIT ALLEN INFORMATIONEN ZU DEN PFLANZEN, INFEKTIONSKRANKHEITEN,  EIGENSCHAFTEN UND THERAPIEZIELE.
;@PARAM TYPE: {LIST}, LISTE MIT ALLEN TYPEN.
(DEFUN PRINTKEY(KEYWORD LIS &OPTIONAL (TYPE '(NAME VIREN BAKTERIEN INFEKTIONSKRANKHEITEN EIGENSCHAFTEN VERRINGERN VERBESSERN)))
(COND((NULL TYPE) (FORMAT T "~%----------------------------------------~%"))
	(T (HANDLER-CASE (PRINTKEY-HELPER KEYWORD (REMOVE-DUPLICATES (APPLY #'APPEND (MAPCAR (LAMBDA (ROW) (CDR (ASSOC (CAR TYPE) ROW))) LIS))) (CAR TYPE))(ERROR (C)(FORMAT T "PROBLEM GEFANGEN~%")))
	(PRINTKEY KEYWORD LIS (CDR TYPE)))))


;FUNCTION PRINTKEY-HELPER
;SUCHT NACH STICHWORT IN DER DATENBANK UND PRINTS ALLE PARTIELLEN TREFFEN
;@PARAM KEYWORD: {STRING}, GESUCHTEN STICHWORT
;@PARAM LIS: {LIST}, LISTE MIT ALLEN INFORMATIONEN ZU DEN PFLANZEN, INFEKTIONSKRANKHEITEN,  EIGENSCHAFTEN UND THERAPIEZIELE.
;@PARAM TYPE: {STRING}, NAME DES GESUCHTEN TYP ('VERRINGERN, 'VERBESSERN, 'EIGENSCHAFTEN, 'NAME, 'BAKTERIEN , 'INFEKTIONSKRANKHEITEN ODER 'VIREN).
(DEFUN PRINTKEY-HELPER(KEYWORD LIS TYPE)
(LET*((ITEM (CAR LIS))(FOUND (SEARCH (STRING KEYWORD)(STRING (CAR LIS)) :TEST 'CHAR=)))
(COND((NULL LIS) NIL)
	((EQUAL KEYWORD 'HP) (START))
	((AND (EQL TYPE 'NAME) FOUND)(FORMAT T "~%~A IST EINE ANTIBAKTERIELL UND ANTIVIRAL WIRKENDE PFLANZE MIT ANTIOXIDATIVEN EIGENSCHAFTEN."ITEM )(PRINTKEY-HELPER KEYWORD (CDR LIS) TYPE))
	((AND (EQL TYPE 'VIREN) FOUND)(FORMAT T "~%~A WIRD DURCH EINEN VIRUS VERSURSACHT" ITEM)(PRINTKEY-HELPER KEYWORD (CDR LIS) TYPE))
	((AND (EQL TYPE 'INFEKTIONSKRANKHEITEN) FOUND)(FORMAT T "~%~A WIRD DURCH EINEN KRANKHEIT VERSURSACHT" ITEM)(PRINTKEY-HELPER KEYWORD (CDR LIS) TYPE))
	((AND (EQL TYPE 'BAKTERIEN) FOUND)(FORMAT T "~%~A WIRD DURCH EINEN BAKTERIEN VERSURSACHT" ITEM)(PRINTKEY-HELPER KEYWORD (CDR LIS) TYPE))
	((AND (EQL TYPE 'EIGENSCHAFTEN) FOUND)(FORMAT T "~%~A IST EINE EIGENSCHAFT VON ~{~A~^, ~}." ITEM (APPLY #'APPEND (MAPCAR (LAMBDA (ROW)(IF (FIND (CAR LIS) (CDR (ASSOC 'EIGENSCHAFTEN ROW))) (LIST(SECOND (ASSOC 'NAME ROW))))) PLANTS)))(PRINTKEY-HELPER KEYWORD (CDR LIS) TYPE))
	((AND (EQL TYPE 'VERRINGERN) FOUND)(FORMAT T "~%~{~A~^, ~} VERRINGERT ~A" (APPLY #'APPEND (MAPCAR (LAMBDA (ROW)(IF (FIND (CAR LIS) (CDR (ASSOC 'VERRINGERN ROW))) (LIST(SECOND (ASSOC 'NAME ROW))))) PLANTS)) ITEM)(PRINTKEY-HELPER KEYWORD (CDR LIS) TYPE))
	((AND (EQL TYPE 'VERBESSERN) FOUND)(FORMAT T "~%~{~A~^, ~} VERBESSERT ~A" (APPLY #'APPEND (MAPCAR (LAMBDA (ROW)(IF (FIND (CAR LIS) (CDR (ASSOC 'VERBESSERN ROW))) (LIST(SECOND (ASSOC 'NAME ROW))))) PLANTS)) ITEM)(PRINTKEY-HELPER KEYWORD (CDR LIS) TYPE))
	(T (PRINTKEY-HELPER KEYWORD (CDR LIS) TYPE))
)))

