(DEFUN ZIELSEARCH (LIS)
	(FORMAT T "~%========================================") 
	(FORMAT T "~%AUSWAHL DES THERAPIEZIELS. : WAEHLEN SIE EIN ZIEL DURCH EINGABE EINER ZAHL 1 ... 4")   
	(FORMAT T "~%1. VERRINGERUNG VON SYMPTOMEN~%2. VERBESSERUNG VON FUNKTIONEN DES ORGANISMUS~%3. ALLE THERAPIEZIELE ANSEHEN~%4. ZURUECK ZUM HAUPTMENUE")
